<?xml version="1.0"?>
<launch>
    <arg name="num_robots" />
    <arg name="wireless_range" />
    <arg name="output" default="log" />
    <arg name="use_sim_time" default="true" />
    <arg name="communication_range" default="-1" />
    <!--<arg name="log_path"/>-->
     
    <node pkg="fake_network" type="fake_network" name="fake_network" output="$(arg output)" required="true">
        <param name="num_robots" value="$(arg num_robots)" />
        <param name="use_sim_time" value="$(arg use_sim_time)" />
        <param name="communication_range"  value="$(arg communication_range)" />
    </node>

</launch>
